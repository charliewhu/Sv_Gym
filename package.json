{
	"name": "sv-gym",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"migrate:deploy": "npx prisma migrate deploy",
		"build": "npx prisma generate && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "pnpm docker:up && sleep 1 && pnpm prisma migrate reset --force && playwright test",
		"test:down": "pnpm docker:up && pnpm prisma migrate deploy && playwright test && pnpm docker:down",
		"test:unit": "vitest --watch=false",
		"test:unit:watch": "vitest --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.3",
		"@playwright/test": "^1.34.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.18.0",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.14.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"sveltekit-superforms": "^0.8.7",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.14.1",
		"daisyui": "^2.51.6",
		"dotenv": "^16.0.3",
		"zod": "^3.21.4"
	}
}
